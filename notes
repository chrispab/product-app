<div class="row">

	<?php
		if ($errors) {
			echo '<ul class="errors">';
			foreach ($errors as $field => $error) {
				echo '<li>' . htmlentities($error) . '</li>';
			}
			echo '</ul>';
		}
	?>
</div>

